{
  "name": "idiot-dashboard",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "expo lint",
    "webbuild": "npx expo export -p web",
    "webdeploy": "npx expo export -p web && eas deploy",
    "webprod": "npx expo export -p web && eas deploy --prod",
    "webserve": "npx expo export -p web && npx expo serve",
    "rec": "adb reverse tcp:8081 tcp:8081",
    "depcheck": "npx depcheck",
    "updateversion": "npx react-native-version --never-amend",
    "build": "cd android && gradlew assembleRelease && cd ..",
    "prebuild": "npx expo prebuild",
    "prebuildc": "npx expo prebuild --clean",
    "clean": "cd android && gradlew clean && cd ..",
    "deploy": "cd android && gradlew assembleRelease && adb install -r app/build/outputs/apk/release/app-release.apk",
    "installapp": "adb install -r android/app/build/outputs/apk/release/app-release.apk"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@react-navigation/bottom-tabs": "^7.4.0",
    "@react-navigation/elements": "^2.6.3",
    "@react-navigation/native": "^7.1.8",
    "expo": "~54.0.25",
    "expo-constants": "~18.0.10",
    "expo-font": "~14.0.9",
    "expo-haptics": "~15.0.7",
    "expo-image": "~3.0.10",
    "expo-linear-gradient": "~15.0.7",
    "expo-linking": "~8.0.9",
    "expo-router": "~6.0.15",
    "expo-splash-screen": "~31.0.11",
    "expo-status-bar": "~3.0.8",
    "expo-symbols": "~1.0.7",
    "expo-system-ui": "~6.0.8",
    "expo-web-browser": "~15.0.9",
    "mqtt": "^5.14.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-gifted-charts": "^1.4.67",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-web": "~0.21.0",
    "react-native-worklets": "0.5.1"
  },
  "devDependencies": {
    "@types/react": "~19.1.0",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~10.0.0",
    "typescript": "~5.9.2"
  },
  "private": true
}
